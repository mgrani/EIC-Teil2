<span class="kw2">xlim</span><span class="br0">(</span><span class="br0">[</span><span class="nu0">0</span> N-<span class="nu0">1</span><span class="br0">]</span><span class="br0">)</span>
<span class="kw2">set</span><span class="br0">(</span><span class="kw2">gca</span>,<span class="co2">'XTick'</span>, <span class="br0">[</span><span class="nu0">0</span> : <span class="nu0">1</span>/<span class="nu0">8</span> : <span class="nu0">1</span><span class="br0">]</span>*<span class="br0">(</span>N-<span class="nu0">1</span><span class="br0">)</span><span class="br0">)</span>
<span class="kw2">set</span><span class="br0">(</span><span class="kw2">gca</span>,<span class="co2">'XTickLabel'</span>,<span class="co2">'0| | | | | | | |N-1'</span><span class="br0">)</span>
<span class="kw2">grid</span> on
<span class="kw2">ylabel</span><span class="br0">(</span><span class="co2">'amplitude'</span><span class="br0">)</span>
<span class="kw2">xlabel</span><span class="br0">(</span><span class="co2">'samples'</span><span class="br0">)</span>
<span class="kw2">title</span><span class="br0">(</span><span class="co2">'Window function (Hann)'</span><span class="br0">)</span>
 
